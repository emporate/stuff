local totalTimeInGameLabel = Instance.new("TextLabel")
totalTimeInGameLabel.Size = UDim2.new(1, 0, 0, 30)
totalTimeInGameLabel.Position = UDim2.new(0, 0, 0, 130)
totalTimeInGameLabel.BackgroundTransparency = 1
totalTimeInGameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
totalTimeInGameLabel.TextStrokeTransparency = 0
totalTimeInGameLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
totalTimeInGameLabel.Font = Enum.Font.SourceSansBold
totalTimeInGameLabel.TextSize = 18
totalTimeInGameLabel.Text = "Total Time in Game:"
totalTimeInGameLabel.Parent = frame

local totalTimeInGameValueLabel = Instance.new("TextLabel")
totalTimeInGameValueLabel.Size = UDim2.new(1, 0, 0, 30)
totalTimeInGameValueLabel.Position = UDim2.new(0, 0, 0, 160)
totalTimeInGameValueLabel.BackgroundTransparency = 1
totalTimeInGameValueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
totalTimeInGameValueLabel.TextStrokeTransparency = 0
totalTimeInGameValueLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
totalTimeInGameValueLabel.Font = Enum.Font.SourceSansBold
totalTimeInGameValueLabel.TextSize = 18
totalTimeInGameValueLabel.Text = "00:00:00"
totalTimeInGameValueLabel.Parent = frame

local initialTotalTime = player.leaderstats.Minutes.Value

local function updateTime()
    local currentTotalTime = player.leaderstats.Minutes.Value
    local totalGainedTime = currentTotalTime - initialTotalTime
    
    timeLabel.Text = string.format("%d minutes", currentTotalTime)
    totalGainedTimeLabel.Text = string.format("%d minutes", totalGainedTime)
end

updateTime()

player.leaderstats.Minutes.Changed:Connect(updateTime)

local function formatTime(seconds)
    local hours = math.floor(seconds / 3600)
    local minutes = math.floor((seconds % 3600) / 60)
    local seconds = seconds % 60
    return string.format("%02d:%02d:%02d", hours, minutes, seconds)
end

local startTime = tick()
local function updateTotalTimeInGame()
    while true do
        local elapsedTime = tick() - startTime
        totalTimeInGameValueLabel.Text = formatTime(math.floor(elapsedTime))
        wait(1)
    end
end

spawn(updateTotalTimeInGame)

local antiAfkButton = Instance.new("TextButton")
antiAfkButton.Size = UDim2.new(0, 150, 0, 30)
antiAfkButton.Position = UDim2.new(0.5, -75, 1, -40)
antiAfkButton.AnchorPoint = Vector2.new(0.5, 1)
antiAfkButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
antiAfkButton.TextColor3 = Color3.fromRGB(255, 255, 255)
antiAfkButton.Font = Enum.Font.SourceSansBold
antiAfkButton.TextSize = 18
antiAfkButton.Text = "Anti-AFK"
antiAfkButton.Parent = frame

antiAfkButton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/RTrade/Voidz/main/AntiAFK.lua'),true))()
end)
